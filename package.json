{
  "name": "mw.js",
  "version": "0.1.7",
  "description": "",
  "main": "dist/main.js",
  "files": [
    "dist/**",
    "!dist/demo/**"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/ --ext .js,.ts",
    "lint:fix": "eslint src/ --fix --ext .js,.ts",
    "prepare": "husky install",
    "prepush": "npm run lint && rimraf dist && tsc --declaration",
    "postupdate": "git push origin main && git push --tags --no-verify && npm publish -f",
    "update:major": "npm version major -m \"chore: set version to %s\" && npm run postupdate",
    "update:minor": "npm version minor -m \"chore: set version to %s\" && npm run postupdate",
    "update:patch": "npm version patch -m \"chore: set version to %s\" && npm run postupdate"
  },
  "keywords": [],
  "author": "bitomic <alehandro.rzrv@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitomic/mwjs.git"
  },
  "homepage": "https://github.com/bitomic/mwjs#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "fs-extra": "^10.0.0",
    "is-stream": "^2.0.1",
    "node-fetch": "^2.6.1",
    "request": "^2.88.2",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.4.3",
    "@types/node-fetch": "^2.5.12",
    "@types/request": "^2.48.6",
    "@types/signale": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "husky": "^7.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  }
}
